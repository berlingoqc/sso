{
  "name": "@berlingoqc/sso",
  "version": "1.3.1",
  "engines": {
    "node": ">=8.9"
  },
  "bin": {
    "sso-create-admin": "/dist/create-admin.js",
    "sso-migrate": "/dist/migrate.js",
    "sso": "/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/berlingoqc/sso"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist *.tsbuildinfo",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -f ./docker/sso.Dockerfile -t ghcr.io/berlingoqc/sso/sso:latest --build-arg GITHUB_ACCESS_TOKEN=$GITHUB_ACCESS_TOKEN .",
    "docker:run": "docker run -p 3001:3000 -d ghcr.io/berlingoqc/sso/sso:latest",
    "docker:push": "docker push ghcr.io/berlingoqc/sso/sso",
    "compose:host": "docker-compose -f ./docker/host.docker-compose.yml",
    "compose:test": "docker-compose -f ./docker/test.docker-compose.yml",
    "migrate": "node ./dist/migrate",
    "prestart": "npm run build",
    "start": "node -r source-map-support/register ./index.js"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@berlingoqc/lb-extensions": "^1.1.0",
    "@loopback/authentication": "^10.0.0",
    "@loopback/authorization": "^0.13.0",
    "@loopback/boot": "^6.0.0",
    "@loopback/context": "^6.0.0",
    "@loopback/core": "^5.0.0",
    "@loopback/metrics": "^0.12.0",
    "@loopback/openapi-v3": "^9.0.0",
    "@loopback/repository": "^6.0.0",
    "@loopback/rest": "^13.0.0",
    "@loopback/rest-explorer": "^6.0.0",
    "@loopback/security": "^0.9.0",
    "@loopback/service-proxy": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "isemail": "^3.2.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "loopback-connector": "^5.0.1",
    "loopback-connector-postgresql": "^5.5.0",
    "node-otp": "^1.2.3",
    "twilio": "^4.11.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^10.0.0",
    "@loopback/eslint-config": "^14.0.0",
    "@loopback/testlab": "^6.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/dot": "^1.1.5",
    "@types/lodash": "^4.14.153",
    "@types/node": "^14.18.47",
    "@types/nodemailer": "^6.4.4",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-eslint-plugin": "^5.0.8",
    "eslint-plugin-mocha": "^10.1.0",
    "newman": "^5.1.0",
    "source-map-support": "^0.5.21",
    "typescript": "~4.9.5"
  }
}
